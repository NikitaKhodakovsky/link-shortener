{
	"name": "ui",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "turbo generate:fetchers && tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"prune": "turbo prune ui --docker --out-dir ./pruned/ui",
		"generate:fetchers": "npx openapi-codegen gen auth && npx openapi-codegen gen links",
		"generate:schema:auth": "curl auth-service/api/auth/swagger-yaml --output ./swagger.auth.yaml",
		"generate:schema:links": "curl link-service/api/links/swagger-yaml --output ./swagger.links.yaml",
		"proxy:start": "node ./proxy.js &",
		"proxy:stop": "kill-port 80",
		"proxy:restart": "npm run proxy:stop && npm run proxy:start"
	},
	"dependencies": {
		"@app/react-auth": "*",
		"@app/url-builder": "*",
		"@tanstack/react-query": "^5.21.7",
		"chart.js": "^4.4.1",
		"formik": "^2.4.5",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.12",
		"react-hot-toast": "^2.4.1",
		"react-paginate": "^8.2.0",
		"react-router-dom": "^6.22.1",
		"react-theme-lib": "^1.0.3",
		"yup": "^1.3.3"
	},
	"devDependencies": {
		"@openapi-codegen/cli": "^2.0.0",
		"@openapi-codegen/typescript": "^8.0.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@svgr/plugin-svgo": "^8.1.0",
		"@tanstack/react-query-devtools": "^5.24.0",
		"@types/node": "^20.11.19",
		"@types/react": "^18.2.56",
		"@types/react-dom": "^18.2.19",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@vitejs/plugin-react": "^4.2.1",
		"browserslist-to-esbuild": "^2.1.1",
		"eslint": "^8.56.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"http-proxy": "^1.18.1",
		"kill-port": "^2.0.1",
		"sass": "^1.71.1",
		"typescript": "^5.2.2",
		"vite": "^5.1.4",
		"vite-plugin-inspect": "^0.8.3",
		"vite-plugin-svgr": "^4.2.0",
		"vite-tsconfig-paths": "^4.3.1"
	},
	"browserslist": [
		"iOS >= 13",
		"Android >= 4.4",
		"last 2 versions",
		"> 0.2%",
		"not dead"
	]
}